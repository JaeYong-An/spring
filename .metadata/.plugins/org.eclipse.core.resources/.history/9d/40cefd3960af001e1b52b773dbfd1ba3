package com.myweb.www.domain;

public class FileVO {

	create table file(
			uuid varchar(256),
			save_id varchar(256),
			file_name varchar(256),
			file_type tinyint(1),
			bno bigint,
			file_size bigint,
			reg_at dateTime default now(),
			primary key(uuid));
	
	private String uuid, saveId, fileName, regAt;
	private long bno, fileSize;
	private int fileType;
}
